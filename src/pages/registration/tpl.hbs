<div class="wrapper__registration">
	<div class="registration">
		<form class="chat__form form">
			<h2 class="title">{{title}}</h2>
			<div class="form-control">
				{{titleEmail}}
				{{{inputEmail}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titleLogin}}
				{{{inputLogin}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titleFirstName}}
				{{{inputFirstName}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titleSecondName}}
				{{{inputSecondName}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titlePhone}}
				{{{inputPhone}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titlePassword}}
				{{{inputPassword}}}
				<small class="error"></small>
			</div>
			<div class="form-control">
				{{titlePasswordAgain}}
				{{{inputPasswordAgain}}}
				<small class="error"></small>
			</div>
			{{{buttonBlue}}}
		</form>
		<div>
			{{{buttonText}}}
		</div>
	</div>
</div>